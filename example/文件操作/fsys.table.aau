//配置表范例
import fsys.table;

setting = fsys.table("/mytable.txt") 
setting.load(); //从文件载入表

//读写成员,象普通table 对象一样使用
setting.a = 123;
setting.b = 456; 

//混入成员到配置表
setting.mixin(
	c = {a=1;b=2};
	d = 123
)

//保存配置表
setting.save()

setting2 = fsys.table("/mytable.txt")
setting2.load(); //读取

io.open() 
for(k,v in setting2){
	io.print(k,v ) //象普通table对象一样使用 
} 

io.print( setting2 ) //转换为自字符串
