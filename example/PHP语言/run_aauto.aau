//在PHP中自由调用aardio语言

import php;

//打开控制台
io.open() ;

//PHP代码
phpcode =/* 
	$ret = aauto("
		import win;
		win.msgbox('我是aardio代码');
		return 123;
	")
*/ 

//运行PHP代码,返回表达式的值
